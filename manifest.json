{
  "name": "3read",
  "version": "0.0.1",
  "description": "3read allows you to read selected text in 3 dimensions.",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],

  "background": {
    "scripts": [
      "keycodes.js",
      "tabs.js",
      "background.js"
    ]
  },

  "browser_action": {
    "default_icon": "icon_19.png",
    "default_title": "3read Selection"
  },

  "options_page": "options.html",

  "minimum_chrome_version": "18",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "keycodes.js",
        "content_script.js"
      ]
    }
  ],

  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },

  "manifest_version": 2
}
